{{#each data}}

  <tr>

    <td>{{this.isbn}}</td>
    <td>{{this.title}}</td>
    <td>{{this.quantity}}</td>
    <form action="/admin/admin-dashboard/removeBooks" method="post">
    <td><input type="number" name="quantity" placeholder="Quantity"/></td>
    <td><button onclick="removeFn({{this.isbn}})">Remove</button></td>
    </form>
  </tr>

{{/each}}